{
  "componentName": "BaseLaunchButton",
  "import": [
    {
      "name": "queryParameters",
      "from": "../../helpers/Functions/url-parameters",
      "type": "package",
      "props": []
    },
    {
      "name": "setQuery",
      "from": "../../helpers/Functions/url-parameters",
      "type": "package",
      "props": []
    },
    {
      "name": "deleteQuery",
      "from": "../../helpers/Functions/url-parameters",
      "type": "package",
      "props": []
    },
    {
      "name": "baseAccessToken",
      "from": "../../helpers/Functions/base-auth",
      "type": "package",
      "props": []
    },
    {
      "name": "baseRefreshToken",
      "from": "../../helpers/Functions/base-auth",
      "type": "package",
      "props": []
    }
  ],
  "renderBeforeReturn": [
  ],
  "lifeCycleMethods": [
    {
      "methodName": "componentDidMount",
      "code": "const qs = queryParameters();if(qs !== null && qs.code !== undefined)this.setState({code: qs.code});this.getAccessToken();"
    }
  ],
  "tags": [
    {
      "componentMethod": {
        "states": [
          { "name": "code", "initial": "\"\"" },
          { "name": "accessToken", "initial": "\"\"" },
          { "name": "refreshToken", "initial": "\"\"" }
        ],
        "methods": [
          { "name": "getAccessToken", "bind": false, "methodType": "plane-async", "args": [], "code": "let qs = queryParameters();if (qs === null || qs.code === undefined) return;await baseAccessToken(qs.code).then(data => {console.log(data);this.setState({ accessToken: data.accessToken });this.setState({ refreshToken: data.refreshToken });});await baseRefreshToken(this.state.refreshToken).then(data => {console.log(data);});deleteQuery([\"code\"]);setQuery({\"at\": this.state.accessToken, \"rt\": this.state.refreshToken});" }
        ]
      }
    },
    {
      "tag": "p",
      "rawContent": "yes",
      "content": "CODE:&nbsp;<strong>{this.state.code}</strong>"
    },
    {
      "tag": "div",
      "props": ["className='text-center mt-5 m-3'"],
      "child": {
        "componentMethod": {
          "states": [
            { "name": "menu", "initial": false }
          ],
          "methods": [
            { "name": "toggle", "bind": true, "methodType": "modal", "state": "menu", "args": [] }
          ]
        },
        "tags": [
          {
            "tag": "a",
            "props": ["href={this.props.href}", "className='btn btn-primary waves-effect waves-light mr-1'"],
            "child": {
              "tags": [
                {
                  "tag": "i",
                  "noCR": "yes",
                  "props": ["className='bx bx-landscape font-size-16 align-middle mr-2'"],
                  "contentAT": "'Authorization BASE'"
                }
              ]
            }
          }
        ]
      }
    }
  ]
}
